{
    "compilerOptions": {
        "target": "es5",
        "declaration": true,
        "module": "commonjs",
        "lib": [
            "es6",
            "dom"
        ],
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "jsx": "react",
        "outDir": "dist"
    },
    "assets":["src/components/css/*"],
    "include": [
        "src/**/*"
    ]
}